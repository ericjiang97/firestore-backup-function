{
  "name": "functions",
  "scripts": {
    "start": "functions-framework --target=backup",
    "build": "tsc",
    "lint": "tslint --project tsconfig.json",
    "predeploy": "npm run lint",
    "deploy": "tsc && gcloud functions deploy backup --trigger-http --runtime nodejs10 --region asia-east2 --env-vars-file .env.yaml"
  },
  "engines": {
    "node": "10"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/functions-framework": "^1.3.2",
    "axios": "^0.19.0",
    "express": "^4.17.3",
    "googleapis": "^44.0.0",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@types/express": "^4.0.0",
    "@types/node": "^12.12.0",
    "dotenv": "^8.2.0",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "private": true
}
